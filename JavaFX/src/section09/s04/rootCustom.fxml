<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>


<HBox spacing="10.0" xmlns:fx="http://javafx.com/fxml" fx:controller="section09.s04.CustomController">
   <children>
      <Button  onAction="#handelOpenFileChooser" text="Open FileChooser" />
      <Button  onAction="#handleSaveFileChooser" text="Save FileChooser" />
      <Button  onAction="#handleDirectoryChooser" text="Directory FileChooser" />
      <Button fx:id="btnPopup"  onAction="#handlePopup" text="Popup" />
      <Button fx:id="btnCustom"  onAction="#handleCustom" text="Custom" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
